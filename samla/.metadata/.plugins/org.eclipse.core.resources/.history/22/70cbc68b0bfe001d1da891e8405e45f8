package com.samla.model;

import java.sql.Timestamp;

import lombok.Data;
@Data
public class ExceptionResponse {
	String msg;
	Timestamp timestamp;
	String path;
	public ExceptionResponse(String msg, Timestamp timestamp, String path) {
		this.msg = msg;
		this.timestamp = timestamp;
		this.path = path;
	}
	
}
